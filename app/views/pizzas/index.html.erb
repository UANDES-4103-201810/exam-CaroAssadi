<p id="notice"><%= notice %></p>

<h1>Pizzas</h1>

<table>
  <thead>

    <tr>
      <th colspan="10"></th>
    </tr>
  </thead>

  <tbody>
  <tr>
    <% @pizzas.each do |pizza| %>
